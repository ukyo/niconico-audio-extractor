{
    "name": "NicoNico Audio Extractor",
    "version": "0.3.4",
    "manifest_version": 2,
    "description": "ニコニコ動画から音声を抽出して保存します。",
    "icons": {
        "48": "img/48.png",
        "128": "img/128.png"
    },
    "background": {
        "page": "html/background.html"
    },
    "permissions": [
        "http://*.nicovideo.jp/*",
        "contextMenus",
        "tabs",
        "notifications"
    ]
}